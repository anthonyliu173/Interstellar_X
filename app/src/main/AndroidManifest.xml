<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anthony.interstellar_x">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/FullscreenTheme"
        android:screenOrientation="portrait">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Levels.Level_1"
            android:screenOrientation="portrait"/>
        <activity android:name=".Levels.Level_2"
            android:screenOrientation="portrait"/>
        <activity android:name=".Levels.Level_3"
            android:screenOrientation="portrait"/>
        <activity android:name=".Levels.Level_4"
            android:screenOrientation="portrait"/>
        <activity android:name=".Levels.Level_5"
            android:screenOrientation="portrait"/>
        <activity android:name=".Levels.Level_6"
            android:screenOrientation="portrait"/>
        <activity android:name=".Levels.Level_7"
            android:screenOrientation="portrait"/>
        <activity android:name=".Levels.Level_8"
            android:screenOrientation="portrait"/>
        <activity android:name=".Levels.Practice"
            android:screenOrientation="portrait"/>
    </application>

</manifest>
